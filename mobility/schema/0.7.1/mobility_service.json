            "$ref" : "https://schema.beckn.org/core/schema/0.7.1/provider.json"
        },
        "trip" : {
            "$ref" : "https://schema.beckn.org/mobility/schema/0.7.1/trip.json"
        },
        "policies" : {
            "type" : "array",
            "items" : {
