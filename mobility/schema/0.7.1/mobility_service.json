{
    "$id": "https://schema.beckn.org/mobility/schema/0.7.1/mobility_service.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "version" : "0.7.1",
    "description": "Describes contents of the mobility service object",
    "type": "object",
    "properties": {
        "id" : {
            "type" : "string"
        },
        "catalog" : {
            "$ref" : "https://schema.beckn.org/core/schema/0.7.1/catalog.json"
        },
        "matched_items" : {
            "type" : "array",
            "items" : {
                "$ref" : "https://schema.beckn.org/core/schema/0.7.1/item.json#/properties/id"
            } 
        },
        "selected_items" : {
            "type" : "array",
            "items" : {
