    "version": "0.5.0",
    "description" : "Describes a telehealth end user app",
    "type": "object",
    "properties": {
        "registration": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "address": {
